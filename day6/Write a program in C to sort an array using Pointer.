#include<stdio.h>
#include<stdlib.h>

int main()
{
int a[20],i,j,n,*ptr,temp;
printf("enter the n value i.e size of array\n");
scanf("%d",&n);
printf("enter the array of elemrnts\n");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("sorted array is\n");
ptr=a;
for(i=1;i<n;i++)
{
	for(j=0;j<n-i;j++)
	{
		if(ptr[j]>ptr[j+1])
		{
		temp=ptr[j];
		ptr[j]=ptr[j+1];
		ptr[j+1]=temp;
		}
	}
}
for(i=0;i<n;i++)
printf("%d\n",ptr[i]);
return 0;
}
