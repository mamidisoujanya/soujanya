#include<stdio.h>
#include<stdlib.h>
int main()
{
int i,j,len,count=0;
char str[100];
printf("enter the string\n");
scanf("%[^\n]%*c",str);
 for (int i = 0; str[i]; i++) 
        if (str[i] != ' ') 
	  {
            str[count] = str[i]; 
                  count++;  
          }               
        str[count] = '\0'; 

printf(" string after removing space %s \n",str);
return 0;
}
