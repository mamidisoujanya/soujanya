#include <iostream>
using namespace std;
 
int main()
{
  char str1[50],str2[50];
   int i=0,j=0;
  cout<<"Enter First String:";
  cin>>str1;
  cout<<"Enter Second String:";
  cin>>str2;
  for(i=0;str1[i]!='\0';i++);
  while(str2[j]!='\0')
  {
    str1[i]=str2[j];
    j++;
    i++;
  }
  str1[i]='\0';
  cout<<"Concatenated String is "<<str1;
return 0;
}
