#include <iostream> 
using namespace std; 
int main()
{
char str[50];
int i,j=0;
cout<<"enter the string\n";
cin>>str;

for(i=0;str[i]!='\0';i++){
if(str[i]>=97&&str[i]<=122)

str[i]=str[i]-32;
else
str[i]=str[i]+32;
}

cout<<"changing the string upper case to lowercase vice versa string is :\n"<<str;
return 0;
}
