#include<stdio.h>
int main()
{
	int decnum,k,i,j,temp;
	char h[100];
  
	for(k=65;k<=90;k++)
	{
	  i=1;
	  decnum=k;
		
	   while(decnum!=0)
    {
    temp = decnum%16;
    
		if( temp<10)
	  temp =temp+48;
		
    else
		 temp = temp+55;
		h[i++]= temp;
		decnum  = decnum/16;
	  }
printf("ascii value of %c is %d\t",k,k);
	printf("hexadecimal value is :");
	for (j = i -1;j>0;j--)
	      printf("%c",h[j]);
		printf("\n\n");
}
	return 0;
}
